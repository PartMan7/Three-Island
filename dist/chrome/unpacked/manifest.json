{
	"name": "Three Island",
	"version": "1.0.3",
	"manifest_version": 3,
	"description": "A Chrome extension for Pokemon Showdown that offers a seamless Pok√©paste experience.",
	"homepage_url": "https://github.com/PartMan7/Three-Island",
	"icons": {
		"48": "icons/three-island-48.png",
		"96": "icons/three-island-96.png",
		"128": "icons/three-island-128.png"
	},
	"permissions": [
		"storage"
	],
	"action": {
		"default_icon": "icons/three-island-48.png",
		"default_title": "Three Island",
		"default_popup": "popup/popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"*://play.pokemonshowdown.com/*",
				"*://*.psim.us/*"
			],
			"js": [
				"syringe.js"
			]
		}
	],
	"host_permissions": [
		"*://play.pokemonshowdown.com/*",
		"*://*.psim.us/*",
		"*://pokepast.es/**"
	],
	"web_accessible_resources": [
		{
			"resources": [
				"three-island.js"
			],
			"matches": [
				"<all_urls>"
			]
		}
	]
}
